043e \u043e\u0447\u0435\u043d\u044c \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u0430\u044f \u0442\u0435\u043c\u0430. \u041a\u0442\u043e \u0431\u043e\u043b\u044c\u0448\u0435 \u0437\u0430\u043f\u043b\u0430\u0442\u0438\u0442 , \u0442\u043e\u0442 \u043f\u0435\u0440\u0432\u044b\u0439 \u0438 \u043f\u0440\u043e\u0435\u0434\u0435\u0442. \u0415\u0449\u0451 \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u0438\u0434\u0443\u043c\u0430\u0442\u044c \u043f\u043b\u0430\u0442\u043d\u0443\u044e \u0433\u043b\u0430\u0432\u043d\u0443\u044e \u0434\u043e\u0440\u043e\u0433\u0443."}, {"cid": 3825516, "uid": 450138380, "from_id": 450138380, "date": 1555406054, "text": "\ud83d\ude01"}, {"cid": 3825531, "uid": 355420568, "from_id": 355420568, "date": 1555406432, "text": "\u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e, \u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u043f\u0435\u0440\u0435\u043a\u0440\u0451\u0441\u0442\u043a\u0438 \u043e\u0447\u0435\u0440\u0435\u0434\u043d\u043e\u0439 \u044d\u0442\u0430\u043f \u0440\u044b\u043d\u043e\u0447\u043d\u044b\u0445 \u0440\u0435\u0444\u043e\u0440\u043c \u0438 \u043d\u0435\u043e\u0442\u044a\u0435\u043c\u043b\u0435\u043c\u0430\u044f \u0447\u0430\u0441\u0442\u044c \u043e\u0431\u0449\u0435\u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u0441\u043a\u0438\u0445 \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439. \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e, \u0432 \u043f\u0435\u0440\u0435\u0434\u043e\u0432\u044b\u0445 \u0441\u0442\u0440\u0430\u043d\u0430\u0445 \u0417\u0430\u043f\u0430\u0434\u0430 (\u0421\u0435\u0432\u0435\u0440\u0430, \u042e\u0433\u0430, \u0412\u043e\u0441\u0442\u043e\u043a\u0430) \u0442\u0430\u043a\u043e\u0439 \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u043e\u043f\u044b\u0442 \u0443\u0436\u0435 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0443\u0435\u0442."}, {"cid": 3825540, "uid": 374922280, "from_id": 374922280, "date": 1555406666, "text": "\u041f\u0440\u0430\u0432\u0434\u0443 \u0433\u043e\u0432\u043e\u0440\u044f\u0442, \u0432 \u0420\u043e\u0441\u0441\u0438\u0438 \u0434\u0432\u0435 \u043e\u0431\u0441\u0443\u0436\u0434\u0430\u0435\u043c\u044b\u0435 \u0442\u0435\u043c\u044b \u0434\u0443\u0440\u0430\u043a\u0438 \u0438 \u0434\u043e\u0440\u043e\u0433\u0438. \u0410 \u0432 \u044d\u0442\u043e \u0441\u043b\u0443\u0447\u0430\u0435 \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u0441\u043a\u0430\u0437\u0430\u0442\u044c:\" \u0414\u0443\u0440\u0430\u043a\u0438(\u043a\u0442\u043e \u0434\u0432\u0438\u0433\u0430\u0435\u0442 \u044d\u0442\u0443 \u0442\u0435\u043c\u0443) + \u0434\u043e\u0440\u043e\u0433\u0438\"."}, {"cid": 3825563, "uid": 12409461, "from_id": 12409461, "date": 1555407140, "text": "\u0418 \u0442\u043e\u043b\u044c\u043a\u043e \u0432 \u0420\u043e\u0441\u0441\u0438\u0438 \u0432\u0441\u0451 \u0434\u0443\u043c\u0430\u044e\u0442 \u0438 \u0440\u0435\u0448\u0430\u044e\u0442, \u043a\u0430\u043a \u0432\u044b\u0436\u0430\u0442\u044c \u0434\u0435\u043d\u044c\u0433\u0438 \u0438\u0437 \u0432\u043e\u0437\u0434\u0443\u0445\u0430... \u041f\u043e\u0431\u043e\u0440\u044b \u0441\u0442\u0430\u043b\u0438 \u0433\u043b\u0430\u0432\u043d\u043e\u0439 \u0440\u0430\u0431\u043e\u0442\u043e\u0439 \u0438 \u0437\u0430\u0431\u043e\u0442\u043e\u0439 \u0434\u0443\u043c\u044b."}, {"cid": 3825646, "uid": 433878161, "from_id": 433878161, "date": 1555409097, "text": "\u041d\u0430\u0441\u0447\u0451\u0442 \u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u043f\u0435\u0440\u0435\u043a\u0440\u0451\u0441\u0442\u043a\u043e\u0432, \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e\u0435 \u0440\u0435\u0448\u0435\u043d\u0438\u0435, \"\u043e\u0434\u043e\u0431\u0440\u044f\u043c\"! \u0421 \u043a\u0430\u0436\u0434\u044b\u043c \u0434\u043d\u0451\u043c \u0436\u0438\u0442\u044c \u0441\u0442\u0430\u043d\u043e\u0432\u0438\u0442\u0441\u044f \u0432\u0441\u0435 \u0432\u0435\u0441\u0435\u043b\u0435\u0439! \u0418 \u043a\u043e\u043c\u0443 \u0441\u043a\u0430\u0437\u0430\u0442\u044c \"\u0441\u043f\u0430\u0441\u0438\u0431\u043e\", \u0447\u0442\u043e \u043d\u0430\u043c \u0441\u0435\u0433\u043e\u0434\u043d\u044f \u0442\u0430\u043a \u0432\u0435\u0441\u0435\u043b\u043e? \u0418 \u0433\u0434\u0435 \u0436\u0435, \u043a\u0430\u043a, \u043d\u0435 \u0432 \u041d\u0438\u0436\u043d\u0435\u043c \u0432\u043d\u0435\u0434\u0440\u0438\u0442\u044c \u044d\u0442\u043e \u0434\u0435\u043b\u043e, \u043d\u0430\u043c \u043d\u0435 \u043f\u0440\u0438\u0432\u044b\u043a\u0430\u0442\u044c \u043a \u044d\u043a\u0441\u043f\u0435\u0440\u0438\u043c\u0435\u043d\u0442\u0430\u043c \u043d\u0430 \u043b\u044e\u0434\u044f\u0445!"}, {"cid": 3825679, "uid": 486961315, "from_id": 486961315, "date": 1555410048, "text": "\u041f\u043b\u0430\u0442\u043d\u044b\u0435 \u043f\u0435\u0440\u0435\u043a\u0440\u0435\u0441\u0442\u043a\u0438- \u0434\u043e\u043b\u0433\u043e  \u0434\u0443\u043c\u0430\u043b\u0438? \u0411\u043e\u043b\u044c\u0448\u0435 \u0443\u0436\u0435 \u043d\u0435\u0447\u0435\u0433\u043e \u043f\u0440\u0438\u0434\u0443\u043c\u0430\u0442\u044c,\u043c\u043e\u0436\u043d\u043e \u0441\u043e\u043a\u0440\u0430\u0442\u0438\u0442\u044c \u043a\u043e\u043b-\u0432\u043e \u0434\u0435\u043f\u0443\u0442\u0430\u0442\u043e\u0432, \u0442\u043e\u043b\u043a\u0443 \u0431\u0443\u0434\u0435\u0442 \u0431\u043e\u043b\u044c\u0448\u0435...\ud83d\ude0a"}, {"cid": 3825726, "uid": 163731450, "from_id": 163731450, "date": 1555411288, "text": "\u041f\u043b\u0430\u0442\u043d\u044b\u0435 \u043f\u0435\u0440\u0435\u043a\u0440\u0451\u0441\u0442\u043a\u0438 - \u044d\u0442\u043e \u0441\u0438\u043b\u044c\u043d\u043e!<br>\u0427\u0442\u043e-\u0442\u043e \u043f\u043e\u0434\u0443\u043c\u0430\u043b\u043e\u0441\u044c \u043e \u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u043f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u044b\u0445 \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u0430\u0445, \u043d\u0435\u0442 \u0434\u0435\u043d\u0435\u0433 - \u0438\u0434\u0438 \u0432 \u043e\u0431\u0445\u043e\u0434....\ud83e\udd14"}, {"cid": 3757238, "uid": 39894790, "from_id": 39894790, "date": 1553207651, "text": "\u0410 \u0432\u0447\u0435\u0440\u0430 \u043f\u0438\u0441\u0430\u043b\u0438 \u0447\u0442\u043e \u0432 \u043f\u043e\u043b\u0438\u0442 \u043f\u0430\u0431\u043b\u0438\u043a\u0430\u0445."}, {"cid": 3757273, "uid": 27511238, "from_id": 27511238, "date": 1553209292, "text": "\u0423\u0427\u0401\u041d\u042b\u0415. \u0412\u044b\u044f\u0441\u043d\u0438\u043b\u0438. . . . \u0421\u043a\u043e\u043b\u043a\u043e\u0432\u043e \u043e\u043f\u044f\u0442\u044c \u043a\u0443\u0447\u0443 \u0434\u0435\u043d\u0435\u0433 \u043d\u0430 \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u043e\u0442\u0440\u0430\u0442\u0438\u043b\u043e..."}, {"cid": 3757278, "uid": 47400180, "from_id": 47400180, "date": 1553209494, "text": "\u0421 \u043a\u0430\u043a\u0438\u0445 \u043f\u043e\u0440 \u0442\u0432\u0438\u0447 \u044d\u0442\u043e \u0441\u0430\u0439\u0442 \u043f\u0440\u043e \u0434\u043e\u0442\u0430 2?  1)\u043c\u0435\u0441\u0442\u043e \u043f\u0438\u043a\u0430\u0431\u0443 2) \u0434\u0432\u0430\u0447 3)\u0442\u0432\u0438\u0447.<br>\u0412\u0438\u0434\u0430\u0442\u044c \u0444\u0430\u043b\u044c\u0441\u0438\u0444\u0438\u043a\u0430\u0446\u0438\u044f \u0444\u0430\u043a\u0442\u043e\u0432 \u044d\u0442\u043e \u043d\u043e\u0440\u043c\u0430 \u0434\u043b\u044f \u0421\u041c\u0418."}, {"cid": 3757310, "uid": 13246383, "from_id": 13246383, "date": 1553212166, "text": "\u0412 \u043f\u0430\u0431\u043b\u0438\u043a\u0435 \"\u041f\u043b\u043e\u0445\u0438\u0435 \u043d\u043e\u0432\u043e\u0441\u0442\u0438\" \u0442\u043e\u0436 \u043d\u0435\u0430\u0434\u0435\u043a\u0432\u0430\u0442 \u0441\u043f\u043b\u043e\u0448\u043d\u043e\u0439 \u0442\u0443\u0441\u0443\u0435\u0442\u0441\u044f."}, {"cid": 3757375, "uid": 401895426, "from_id": 401895426, "date": 1553221664, "text": "\u0417\u0430\u0439\u0434\u0438\u0442\u0435 \u043d\u0430 \u041b\u0435\u043d\u0442\u0430.\u0440\u0443 \u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u0441\u0442\u0443\u043f\u0438\u0442\u0435 \u0432 \u0434\u0438\u0430\u043b\u043e\u0433.\u041c\u043d\u043e\u0433\u043e \u043d\u043e\u0432\u043e\u0433\u043e \u0443\u0437\u043d\u0430\u0435\u0442\u0435 \u0438 \u043e \u0441\u0435\u0431\u0435,\u0438 \u043e \u0447\u043b\u0435\u043d\u0430\u0445 \u0441\u0432\u043e\u0435\u0439 \u0441\u0435\u043c\u044c\u0438.\u0410 \u0435\u0449\u0435 \u043f\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435 \u0441\u0432\u043e\u0439 \u0441\u043b\u043e\u0432\u0430\u0440\u043d\u044b\u0439 \u0437\u0430\u043f\u0430\u0441.\u041f\u043e\u0437\u043d\u0430\u043a\u043e\u043c\u0438\u0442\u0435\u0441\u044c \u0441 \u0432\u043e\u044f\u043a\u043e\u0439,\u043a\u043e\u0442\u043e\u0440\u044b\u0439 \"\u0432\u0441\u044e \u0447\u0435\u0447\u0435\u043d\u0441\u043a\u0443\u044e \u043f\u0440\u043e\u0448\u0435\u043b\"(\u043d\u043e,\u0441\u0443\u0434\u044f \u043f\u043e \u043e\u0448\u0438\u0431\u043a\u0430\u043c,\u043f\u0430\u0440\u043d\u044e \u043b\u0435\u0442 16,\u043d\u0435 \u0431\u043e\u043b\u044c\u0448\u0435),\u0441 \"\u043c\u043e\u0442\u0430\u0432\u0448\u0438\u043c \u0442\u0440\u0438 \u0441\u0440\u043e\u043a\u0430 \u0437\u0430 \u0441\u0432\u043e\u0431\u043e\u0434\u0443\" \u043c\u043e\u043b\u043e\u0434\u044b\u043c \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u043e\u043c,\u043e\u043a\u0430\u0437\u044b\u0432\u0430\u044e\u0449\u0438\u043c \u0430\u0434\u0432\u043e\u043a\u0430\u0442\u0441\u043a\u0438\u0435 \u0443\u0441\u043b\u0443\u0433\u0438,\u043d\u0435 \u0438\u043c\u0435\u044f \u0441\u043f\u0435\u0446. \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044f.\u041a\u0443\u0447\u0430 \u0431\u043e\u0442\u043e\u0432 \u0438 \u0442\u0440\u043e\u043b\u043b\u0435\u0439 \u0432\u0441\u0435\u0445 \u043c\u0430\u0441\u0442\u0435\u0439.\u041d\u0438\u043a\u0430\u043a\u043e\u0433\u043e \u0438\u0441\u0441\u043b\u0435\u0434\u043e\u0432\u0430\u043d\u0438\u044f \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u0442\u044c \u043d\u0435 \u043d\u0443\u0436\u043d\u043e .\u041d\u0430\u0441\u0442\u043e\u044f\u0449\u0438\u0439 \u0437\u043e\u043e\u043f\u0430\u0440\u043a!"}, {"cid": 3757441, "uid": 409169923, "from_id": 409169923, "date": 1553226556, "text": "[id27511238|\u0412\u0430\u0441\u0438\u043b\u0438\u0439], \u0431\u0440\u0438\u0442\u0430\u043d\u0441\u043a\u0438\u0435 \u0443\u0447\u0435\u043d\u044b\u0435 \u0432\u044b\u044f\u0441\u043d\u0438\u043b\u0438 .\ud83d\ude06\ud83d\ude02\ud83e\udd23", "reply_to_uid": 27511238, "reply_to_cid": 3757273}, {"cid": 3757444, "uid": 342310382, "from_id": 342310382, "date": 1553226661, "text": "[id401895426|\u0422\u0430\u0442\u044c\u044f\u043d\u0430], \u043f\u043e \u043c\u043e\u0435\u043c\u0443 \u0432 \u043b\u044e\u0431\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0435 \u0412\u041a \u0442\u0430\u043a \u0436\u0435.\u0415\u0441\u043b\u0438 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043f\u0438\u0448\u0435\u0442 \u0432 \u0440\u0430\u0437\u0440\u0435\u0437 \u0441 \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u043e\u0439 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u044b \u043e\u043d \u043f\u043e\u043b\u0443\u0447\u0438\u0442 \u043c\u043d\u043e\u0433\u043e \u043d\u0435\u043b\u0435\u0441\u0442\u043d\u044b\u0445 \u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0440\u0438\u0435\u0432 \u0432 \u0441\u0432\u043e\u0439 \u0430\u0434\u0440\u0435\u0441.\u0418 \u043d\u0435 \u0432\u0430\u0436\u043d\u043e , \u043f\u0438\u0448\u0435\u0448\u044c \u043b\u0438 \u043d\u0430 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0435 \u043c\u044f\u0441\u043e\u0435\u0434\u043e\u0432 \u0447\u0442\u043e \u0442\u043e \u043e \u043f\u043e\u043b\u044c\u0437\u0435 \u0432\u0435\u0433\u0435\u0442\u0430\u0440\u0438\u0430\u043d\u0441\u0442\u0432\u0430 \u0438\u043b\u0438 \u043d\u0430 \u043f\u0430\u0442\u0440\u0438\u043e\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u043f\u0430\u0431\u043b\u0438\u043a\u0435 \u0441\u043b\u0435\u0433\u043a\u0430 \u043f\u043e\u0445\u0432\u0430\u043b\u0438\u0448\u044c \"\u0437\u0430\u0433\u043d\u0438\u0432\u0430\u044e\u0449\u0438\u0439\" \u0437\u0430\u043f\u0430\u0434.\u0421\u043a\u043e\u0432\u0430\u043d\u044b \u043e\u0434\u043d\u043e\u0439 \u0446\u0435\u043f\u044c\u044e.", "reply_to_uid": 401895426, "reply_to_cid": 3757375}, {"cid": 3757454, "uid": 83681893, "from_id": 83681893, "date": 1553227044, "text": "\u0423\u0447\u0435\u043d\u044b\u0435 \u0435\u0449\u0435 \u043d\u0435 \u0437\u0430\u0445\u043e\u0434\u0438\u043b\u0438 \u043d\u0430 \u043c\u0430\u043c\u0441\u043a\u0438\u0435 \u0441\u0430\u0439\u0442\u044b)),\u0432\u043e\u0442 \u0433\u0434\u0435 \u043c\u043e\u0447\u0438\u043b\u043e\u0432\u043e \u043c\u0435\u0436\u0434\u0443 \u0433\u0432 \u0438 \u0438\u0432, \u0438 \u043f\u0440\u043e \u043f\u0440\u0438\u0432\u0438\u0432\u043a\u0438))"}, {"cid": 3757523, "uid": 166818167, "from_id": 166818167, "date": 1553230257, "text": "\u0421\u0447\u0438\u0442\u0430\u044e,\u043d\u0430\u0434\u043e \u043e\u0442\u0441\u0442\u0430\u0438\u0432\u0430\u0442\u044c \u0441\u0432\u043e\u0435 \u043c\u043d\u0435\u043d\u0438\u0435,\u043d\u043e,\u0435\u0441\u043b\u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u0435\u0442\u0441\u044f \u043c\u0430\u0442 \u0438 \u043e\u0441\u043a\u043e\u0440\u0431\u043b\u0435\u043d\u0438\u044f,\u0442\u043e \u0442\u0430\u043a\u0438\u0445 \u0434\u0435\u0431\u0438\u043b\u043e\u0432\u0430\u0442\u044b\u0445 \u0438 \u0431\u0435\u0437\u0433\u0440\u0430\u043c\u043e\u0442\u043d\u044b\u0445 \u0431\u043b\u0438\u0437\u043a\u043e \u043d\u0435 \u0434\u043e\u043f\u0443\u0441\u043a\u0430\u0442\u044c \u0434\u043e \u043e\u0431\u0441\u0443\u0436\u0434\u0435\u043d\u0438\u0439 ."}, {"cid": 3758520, "uid": 518297048, "from_id": 518297048, "date": 1553253921, "text": "https://vk.com/photo-15722194_456435703"}]